{
  "presets": [
    [
      "@babel/env",
      {
        "exclude": [
          "transform-regenerator",
          "transform-async-to-generator"
        ],
        "modules": false,
        "debug": true,
        "targets": {
          "node": 4
        }
      }
    ]
  ],
  "plugins": [
    [
      "module:fast-async",
      {
        "spec": true
      }
    ],
    [
      "transform-rename-import",
      {
        "original": "wrote",
        "replacement": "wrote/es5"
      }
    ]
  ]
}
